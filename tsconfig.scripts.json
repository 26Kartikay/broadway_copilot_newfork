{
  "compilerOptions": {
    "target": "ES2022",
    "module": "CommonJS",
    "moduleResolution": "Node",
    "outDir": "dist/scripts",
    "strict": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false
  },
  "include": ["scripts/generateEmbeddings.ts", "scripts/importProducts.ts", "scripts/deleteProducts.ts"],
  "exclude": ["node_modules", "dist", "functions", "src", "scripts/addAdmin.ts"]
}

